{
  "name": "@ystv/scores",
  "version": "0.0.0",
  "scripts": {
    "cfg": "node-config-ts",
    "dev": "concurrently -n server,ts,client npm:dev:server npm:dev:server:typecheck npm:dev:client",
    "scores:dev": "yarn dev",
    "dev:server": "node startDevServer.js",
    "dev:server:typecheck": "cd src/server && tsc -w --preserveWatchOutput",
    "dev:client": "vite dev",
    "build": "rm -rf dist && run-p build:client build:server",
    "build:server": "node buildProdServer.js",
    "build:client": "vite build",
    "prod": "concurrently -k -n server,client npm:prod:server npm:prod:client",
    "prod:server": "node --enable-source-maps dist/index.server.js",
    "prod:client": "node serveBuiltClient.js",
    "rls": "esbuild ./src/server/rosesLiveSync.ts --bundle --outfile=./dist/rosesLiveSync.js --platform=node --sourcemap && node --enable-source-maps dist/rosesLiveSync.js $@",
    "test:unit:client": "vitest --run --coverage",
    "test:unit:common": "cd src/common && jest --coverage",
    "test:unit:server": "cd src/server && jest --coverage",
    "test": "run-s --continue-on-error 'test:unit:*'",
    "test:integration:client": "NODE_ENV=test INTEGRATION=true PUBLIC_API_BASE=http://localhost:8000/api vitest --run --coverage --no-threads",
    "test:integration": "run-s --continue-on-error 'test:integration:*'"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.18",
    "@vitejs/plugin-react": "^1.2.0",
    "@vitest/coverage-c8": "^0.26.2",
    "babel-jest": "^27.5.1",
    "concurrently": "^7.0.0",
    "del": "^6.1.1",
    "esbuild": "^0.19.10",
    "happy-dom": "^17.1.8",
    "http-proxy": "^1.18.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "serve-handler": "^6.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.9.4",
    "vite": "^6.2.0",
    "vitest": "^0.26.2"
  },
  "dependencies": {
    "@mantine/core": "^5.10.0",
    "@mantine/dates": "^5.10.0",
