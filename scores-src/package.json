{
  "name": "@ystv/scores",
  "version": "0.0.0",
  "scripts": {
    "cfg": "node-config-ts",
    "dev": "concurrently -n server,ts,client npm:dev:server npm:dev:server:typecheck npm:dev:client",
    "scores:dev": "yarn dev",
    "dev:server": "node startDevServer.js",
    "dev:server:typecheck": "cd src/server && tsc -w --preserveWatchOutput",
    "dev:client": "vite dev",
    "build": "rm -rf dist && yarn build:client && yarn build:server",
    "build:server": "esbuild --platform=node --target=node16 --tree-shaking=true --bundle --sourcemap --outfile=dist/index.server.js src/server/index.server.ts",
    "build:client": "vite build",
    "prod": "concurrently -n server,client npm:prod:server npm:prod:client",
    "prod:server": "node dist/index.server.js",
    "prod:client": "node serveBuiltClient.js"
  },
  "private": true,
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@vitejs/plugin-react": "^1.2.0",
    "concurrently": "^7.0.0",
    "esbuild": "^0.14.23",
    "http-proxy": "^1.18.1",
    "nodemon": "^2.0.15",
    "patch-package": "^6.4.7",
    "serve-handler": "^6.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.4"
  },
  "dependencies": {
    "@mantine/core": "^4.1.2",
    "@mantine/dates": "^4.1.2",
    "@mantine/form": "^4.1.2",
    "@mantine/hooks": "^4.1.2",
    "@mantine/notifications": "^4.1.2",
    "@popperjs/core": "^2.11.2",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-ws": "^3.0.1",
    "@types/http-errors": "^1.8.2",
    "@types/logfmt": "^1.2.2",
    "@types/on-finished": "^2.3.1",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "body-parser": "^1.20.0",
    "conf": "^10.1.1",
    "config": "^3.3.7",
    "convict": "^6.2.1",
    "cors": "^2.8.5",
    "couchbase": "3.2.4",
    "dayjs": "^1.11.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "express-ws": "^5.0.2",
    "fastify": "^3.27.2",
    "formik": "^2.2.9",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "logfmt": "^1.3.2",
    "loglevel": "^1.8.0",
    "loglevel-plugin-prefix": "^0.8.4",
    "on-finished": "^2.4.1",
    "prom-client": "^14.0.1",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6",
    "redis": "^4.0.4",
    "swr": "^1.2.2",
    "tabler-icons-react": "^1.44.0",
    "tiny-invariant": "^1.2.0",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "yup": "^0.32.11"
  },
  "packageManager": "yarn@3.2.0"
}
