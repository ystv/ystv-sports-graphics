{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["es2021", "DOM"],
    "module": "commonjs",
    "target": "es2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "common/*": ["./src/common/*"]
    }
  },
  "exclude": ["cypress/**"]
}
